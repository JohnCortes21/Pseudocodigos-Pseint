Proceso CONVERTIR_GRADOS
	Definir c,f Como entero;
	f<-(c*(9/5))+32;
	Escribir ' BUENAS COMPAÑER@S VAMOS A CONVERTIR GRADOS CENTIGRADOS A GRADOS FARENHEIT ';
	// Aqui iniciamos con el saludo.
	Escribir ' Por favor Ingrese Grados Centigrados ', 'Loading..... ';
	Leer C;
	Escribir ' Su  NUMERO ingresado es= ',C, '°C';
	Escribir ' Los grados centigrados han sido convertidos a Grados  Farenheit. ', ' °F= ',f;
	Escribir 'Gracias por utilizar el algoritmo.';
FinProceso
