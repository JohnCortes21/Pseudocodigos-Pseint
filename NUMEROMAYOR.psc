Proceso MayordedosNumeros
	Definir num1,num2 Como Entero;
	Escribir 'Este es un Programa que calcula el mayor de dos numeros';
	Escribir 'Ingrese el Primer Numero';
	Leer num1;
	Escribir 'Ingrese el Segundo Numero';
	Leer num2;
	Si num1>num2 Entonces
		Escribir ' El numero',num1,' ingresado es Mayor que ',num2;
	SiNo
		Escribir 'El numero',num1,' ingresado es menor que ',num2;
	FinSi
FinProceso
