	DEFINIR I COMO ENTERO;
	DEFINIR PLUS Como Entero;
	Para  I<-1 Hasta 100 Con Paso 1 Hacer
		PLUS=PLUS+I;
		Escribir 'INTERACIÓN NUMERO=', plus;
	Fin Para
